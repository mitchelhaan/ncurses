		do
			echo 'Appending rules for '$cf_item' model ('$cf_dir')'
			
CF_ITEM=`echo $cf_item | tr '[a-z]' '[A-Z]'`

			
	
	case $cf_item in
	normal)  cf_suffix='.a'   ;;
	debug)   cf_suffix='_g.a' ;;
	profile) cf_suffix='_p.a' ;;
